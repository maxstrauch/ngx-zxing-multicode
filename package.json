{
  "name": "ngx-zxing-multicode-demo",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve --port 6767",
    "dev:ssl": "ng serve --ssl --ssl-key localhost.key --ssl-cert localhost.crt --port 6767 --host 0.0.0.0",
    "gen:sscert": "openssl req -new -x509 -newkey rsa:2048 -sha256 -nodes -keyout localhost.key -days 3560 -out localhost.crt -config certificate.cnf",
    "build:patch": "cd projects/ngx-zxing-multicode/ && npm version patch && ng build NgxZxingMulticode --configuration production",
    "build:minor": "cd projects/ngx-zxing-multicode/ && npm version minor && ng build NgxZxingMulticode --configuration production",
    "dist": "cd dist/ngx-zxing-multicode && cp ../../README.md . && npm publish",
    "pub:patch": "npm run build:patch && npm run dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/forms": "~13.2.2",
    "@angular/http": "~7.0.0",
    "@angular/localize": "^13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "core-js": "^2.6.9",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-devkit/core": "^13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5"
  }
}
